n\r\n.welcome {\r\n    background-image: $background-home;\r\n    background-size: cover;\r\n    background-position: center center;\r\n    background-repeat: no-repeat;\r\n    color: $white;\r\n    width: 100%;\r\n    text-align: center;\r\n    height: 60vh;\r\n    padding-top: 3rem;\r\n    overflow: hidden;\r\n\r\n    &__caption {\r\n        font-size: $title;\r\n        letter-spacing: -7px;\r\n        font-weight: 800;\r\n        user-select: none;\r\n    }\r\n\r\n    &__description {\r\n        font-size: $heading;\r\n        font-weight: 300;\r\n        margin-top: -3rem;\r\n    }\r\n\r\n    &__text {\r\n        font-size: $regular;\r\n        font-weight: 300;\r\n        width: 40rem;\r\n        margin-left: auto;\r\n        margin-right: auto;\r\n        margin-top: -.5rem;\r\n    }\r\n    \r\n    &__button {\r\n        background-color: $yellow;\r\n        @include button-style;\r\n        margin-left: auto;\r\n        margin-right: auto;\r\n        margin-top: 2rem;\r\n        width: 300px;\r\n        height: 55px;\r\n        \r\n\r\n\r\n        &::before {\r\n            content: \"\";\r\n            position: absolute;\r\n            width: 100%;\r\n            height: 100%;\r\n            border-radius: 40px;\r\n            background-color: $black;\r\n            z-index: 0;\r\n            transform: scaleX(0);\r\n            transform-origin: right;\r\n            transition: transform .6s;\r\n\r\n        }\r\n\r\n        &:hover::before {\r\n            transform-origin: left;\r\n            transform: scaleX(1);\r\n        }\r\n\r\n        a {\r\n            @include button-text;\r\n            color: $black;\r\n            text-transform: uppercase;\r\n            font-weight: 800;\r\n            font-size: $button-font;\r\n            transition: all .6s;\r\n\r\n\r\n            &:hover {\r\n                color: $white;\r\n            }\r\n            \r\n        }\r\n        \r\n    }\r\n}\r\n\r\n\r\n@media (max-width:1040px) {\r\n    \r\n}","scss\\abstracts\\images.scss":"$background-home: url(\"/assets/Backgrounds/bg-1.png\");\r\n$background-design: url(\"/assets/Backgrounds/bg-2.png\")\r\n\r\n","scss\\abstracts\\button.scss":"\r\n@mixin button-style {\r\n    width: fit-content;\r\n    border-radius: 40px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    position: relative;\r\n    overflow: hidden;\r\n}\r\n\r\n@mixin button-text {\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    text-decoration: none;\r\n    width: 100%;\r\n    height: 100%;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    z-index: 1;\r\n}","scss\\components\\carousel.scss":"\r\n@use 'abstracts/' as *;\r\n\r\n$elements: 12;\r\n$elements-displayed: 4;\r\n$animation-duration: calc($elements * 4s);\r\n\r\n.carousel {\r\n    background-color: $black;\r\n    width: 100%;\r\n    height: 52vh;\r\n    overflow: hidden;\r\n    padding: auto 0;\r\n    position: relative;\r\n\r\n    \r\n\r\n    &__list {\r\n        list-style: none;\r\n        height: 100%;\r\n        display: flex;\r\n        animation: scrolling $animation-duration linear infinite;\r\n\r\n\r\n        &__item {\r\n            width: 40rem;\r\n            flex-shrink: 0;\r\n            white-space: nowrap;\r\n\r\n            img {\r\n                width: 40rem;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n\r\n@keyframes scrolling {\r\n    0% {\r\n        transform: translateX(0);\r\n    }\r\n    100% {\r\n        transform: translateX(-1*30vw*$elements);\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"2dd764d3cc330eab030d1e95b8804e18","cacheData":{"env":{}}}